<% if @test.complete %>
  <div class="no-pad-no-marg c100 large canary-color p<%= @test.score %>">
    <span><%= @test.score %>%</span>
    <div class="slice">
      <div class="bar"></div>
      <div class="fill"></div>
    </div>
  </div><br /><br /><br /><br /><br /><br /><br /><br />
  <% if @test.diffs.blank? %>
    <h4>No detected differences in XML FHIR documents!</h4><br />
  <% else %>
    <h4>Detected differences in XML FHIR documents:</h4><br />
    <%= @test.diffs.html_safe %>
  <% end %>
<% else %>
  <h4 class="mt-3 mb-3">Enter the fields below into the system. When complete, export the record in FHIR format (XML), and upload it. Canary will inspect it for correctness and report the results.</h4>
  <%= form_tag({action: :upload_fhir, test_type: @type, system_id: params['system_id']}, multipart: true) do |f| %>
    <%= file_field_tag 'fhir' %>
    <%= submit_tag "Upload!", class: "btn btn-primary" %>
  <% end %>
  <div class="mb-5"></div>
  <% @test.data.keys.each do |key| %>
    <strong><%= key %></strong>: <%= @test.data[key] %>
    <br /><br />
  <% end %>
<% end %>
